{
  "name": "rserve-ts",
  "version": "0.2.6",
  "description": "",
  "private": false,
  "types": "./dist/index.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "types"
  ],
  "exports": {
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/types": {
      "types": "./dist/types/index.d.ts"
    },
    "./dist/types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "default": "./dist/types/*.js"
    }
  },
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "build": "tsup src/index.ts src/types/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish",
    "rserve-no-oc": "Rscript tests/r_files/no_oc_start.R",
    "rserve-oc": "Rscript tests/r_files/oc_start.R",
    "rserve": "concurrently \"pnpm run rserve-no-oc\" \"pnpm run rserve-oc\""
  },
  "keywords": [],
  "author": "Tom Elliott",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@types/node": "^20.14.1",
    "@types/ws": "^8.5.11",
    "bluebird": "^3.7.2",
    "concurrently": "^8.2.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "ws": "^8.18.0"
  },
  "dependencies": {
    "underscore": "^1.13.6",
    "zod": "^3.23.8"
  }
}
